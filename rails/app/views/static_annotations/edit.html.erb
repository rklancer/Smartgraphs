<h1>Editing static_annotation</h1>

<% form_for(@static_annotation) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :annotation_type %><br />
    <%= f.text_field :annotation_type %>
  </p>
  <p>
    <%= f.label :points_id %><br />
    <%= f.text_field :points_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @static_annotation %> |
<%= link_to 'Back', static_annotations_path %>